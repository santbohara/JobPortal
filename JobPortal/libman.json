{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "jquery@2.2.4",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-icons@1.8.3",
      "destination": "wwwroot/lib/bootstrap-icon/"
    },
    {
      "provider": "cdnjs",
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/"
    },

    {
      "provider": "jsdelivr",
      "library": "font-awesome@latest",
      "destination": "wwwroot/lib/font-awesome/"
    }
  ]
}