{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "jsdelivr",
      "library": "jquery@2.2.4",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/"
    },
    {
      "provider": "unpkg",
      "library": "@fortawesome/fontawesome-free@6.1.1",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap-select@1.14.0-beta2",
      "destination": "wwwroot/lib/bootstrap-select/",
      "files": [
        "dist/css/bootstrap-select.css",
        "dist/css/bootstrap-select.css.map",
        "dist/css/bootstrap-select.min.css",
        "dist/js/bootstrap-select.js",
        "dist/js/bootstrap-select.js.map",
        "dist/js/bootstrap-select.min.js",
        "dist/js/bootstrap-select.min.js.map"
      ]
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/"
    },
    {
      "library": "tinymce@6.1.0",
      "destination": "wwwroot/lib/tinymce/"
    }
  ]
}